{
  "name": "claudectl",
  "version": "1.0.0",
  "description": "Multi-agent Claude orchestration CLI",
  "type": "module",
  "bin": {
    "claudectl": "./bin/claudectl.js"
  },
  "main": "./dist/cli.js",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node bin/claudectl.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix && prettier --write src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "claude",
    "anthropic",
    "ai",
    "cli",
    "multi-agent",
    "orchestration"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@inkjs/ui": "^2.0.0",
    "chalk": "^5.4.1",
    "conf": "^13.1.0",
    "execa": "^9.5.2",
    "ink": "^5.1.0",
    "meow": "^13.2.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.18",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.7.4",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.51.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18"
  }
}
